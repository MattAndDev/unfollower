{
  "name": "Unfollower",
  "description": "Discover what users are not following you back on Instagram",
  "version": "0.1.0",
  "manifest_version": 3,
  "permissions": ["storage", "tabs"],
  "icons": {
    "16": "assets/icon-16.png",
    "32": "assets/icon-32.png",
    "48": "assets/icon-48.png",
    "128": "assets/icon-128.png"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["https://www.instagram.com/*"],
      "js": ["./script.js"]
    },
    {
      "matches": ["https://www.instagram.com/*"],
      "js": ["./script.js"]
    }
  ]
}
